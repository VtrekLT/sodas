<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.bakis">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListActivity"
            android:parentActivityName=".InfoActivity"
            android:label="Augalų sąrašas"></activity>
        <activity android:name=".Plot"
            android:parentActivityName=".InfoActivity"
            android:label="Sodo planas"/>
        <activity
            android:name=".NewPlantActivity"
            android:label="Sukurti užrašą"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".PlantsActivity" />
        <activity android:name=".Planttas" />
        <activity android:name=".Login" />
        <activity android:name=".Register">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".InfoActivity"
            android:label="@string/app_name"></activity>
        <activity android:name=".MainActivity"
            android:label="Užrašinė"
            android:parentActivityName=".InfoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>